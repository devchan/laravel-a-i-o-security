{
  "name": "devchan/laravel-a-i-o-security",
  "description": "This is Laravel all in one security package",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "devchan",
      "email": "chanakadesilva21@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "illuminate/auth": "~5.6.0",
    "illuminate/config": "~5.6.0",
    "illuminate/support": "~5.6.0",
    "illuminate/filesystem": "~5.6.0",
    "ezyang/htmlpurifier": "4.10.*"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^6.3",
    "laravel/passport": "^5.0|^6.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.6",
    "phpunit/phpunit": "^7.0",
    "codeclimate/php-test-reporter": "0.4.*",
    "doctrine/instantiator": "1.0.*",
    "graham-campbell/testbench": "^3.2",
    "scrutinizer/ocular": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Devchan\\LaravelAIOSecurity\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Devchan\\LaravelAIOSecurity\\LaravelAIOSecurityServiceProvider"
      ],
      "aliases": {
        "DevchanAIOSecurity": "Devchan\\LaravelAIOSecurity\\Facade\\LaravelAIOSecurity"
      }
    }
  }
}
